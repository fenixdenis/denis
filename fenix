from tkinter import *
import random

tk = Tk()
canvas = Canvas(tk, width=500, height=500)
canvas.pack()

def random_rectangle(width, heihg, fill_color):
    x1 = random.randrange(width)
    y1 = random.randrange(heihg)
    x2 = random.randrange(x1 + random.randrange(width))
    y2 = random.randrange(y1 + random.randrange(heihg))
    canvas.create_rectangle(x1, y1, x2, y2, fill=fill_color)
a = ['green','red','blue','orange','yellow']
for i in range(100):
    random_rectangle(400,700,random.choice(a))


mainloop()

print('hello world')
